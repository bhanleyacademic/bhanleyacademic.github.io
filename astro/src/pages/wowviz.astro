<div class="highlight hl-ipython2"><pre><span></span><span class="c1"># Function to format the cost</span>
<span class="k">def</span> <span class="nf">format_cost</span><span class="p">(</span><span class="n">cost</span><span class="p">):</span>
    <span class="c1"># Check if the cost is already a string in the format 'Xg Ys Zc'</span>
    <span class="k">if</span> <span class="nb">isinstance</span><span class="p">(</span><span class="n">cost</span><span class="p">,</span> <span class="nb">str</span><span class="p">)</span> <span class="ow">and</span> <span class="s1">'g'</span> <span class="ow">in</span> <span class="n">cost</span> <span class="ow">and</span> <span class="s1">'s'</span> <span class="ow">in</span> <span class="n">cost</span> <span class="ow">and</span> <span class="s1">'c'</span> <span class="ow">in</span> <span class="n">cost</span><span class="p">:</span>
        <span class="k">return</span> <span class="n">cost</span>

    <span class="c1"># Convert the cost to a float</span>
    <span class="n">cost</span> <span class="o">=</span> <span class="nb">float</span><span class="p">(</span><span class="n">cost</span><span class="p">)</span>

    <span class="c1"># Format the cost to 4 decimal places</span>
    <span class="n">formatted_cost</span> <span class="o">=</span> <span class="sa">f</span><span class="s2">"</span><span class="si">&#123;</span><span class="n">cost</span><span class="si">:</span><span class="s2">.4f</span><span class="si">}</span><span class="s2">"</span>

    <span class="c1"># Split the cost into gold, silver, and copper</span>
    <span class="n">gold</span> <span class="o">=</span> <span class="nb">int</span><span class="p">(</span><span class="nb">float</span><span class="p">(</span><span class="n">formatted_cost</span><span class="p">))</span>
    <span class="n">silver</span> <span class="o">=</span> <span class="nb">int</span><span class="p">((</span><span class="nb">float</span><span class="p">(</span><span class="n">formatted_cost</span><span class="p">)</span> <span class="o">*</span> <span class="mi">100</span><span class="p">)</span> <span class="o">%</span> <span class="mi">100</span><span class="p">)</span>
    <span class="n">copper</span> <span class="o">=</span> <span class="nb">int</span><span class="p">((</span><span class="nb">float</span><span class="p">(</span><span class="n">formatted_cost</span><span class="p">)</span> <span class="o">*</span> <span class="mi">10000</span><span class="p">)</span> <span class="o">%</span> <span class="mi">100</span><span class="p">)</span>

    <span class="c1"># Format gold with commas</span>
    <span class="n">gold_str</span> <span class="o">=</span> <span class="sa">f</span><span class="s2">"</span><span class="si">&#123;</span><span class="n">gold</span><span class="si">:</span><span class="s2">,</span><span class="si">}</span><span class="s2">"</span>

    <span class="c1"># Format silver and copper with leading zeros</span>
    <span class="n">silver_str</span> <span class="o">=</span> <span class="sa">f</span><span class="s2">"</span><span class="si">&#123;</span><span class="n">silver</span><span class="si">:</span><span class="s2">02</span><span class="si">}</span><span class="s2">"</span>
    <span class="n">copper_str</span> <span class="o">=</span> <span class="sa">f</span><span class="s2">"</span><span class="si">&#123;</span><span class="n">copper</span><span class="si">:</span><span class="s2">02</span><span class="si">}</span><span class="s2">"</span>

    <span class="k">return</span> <span class="sa">f</span><span class="s2">"</span><span class="si">&#123;</span><span class="n">gold_str</span><span class="si">}</span><span class="s2">g </span><span class="si">&#123;</span><span class="n">silver_str</span><span class="si">}</span><span class="s2">s </span><span class="si">&#123;</span><span class="n">copper_str</span><span class="si">}</span><span class="s2">c"</span>

<span class="c1"># Function to generate hex positions and chart</span>
<span class="c1"># @param df_subset: DataFrame subset to generate heatmap</span>
<span class="c1">#                   Must contain columns: Recipe Name, Recipe Cost, Sale Price, Profit</span>
<span class="c1"># @param title: Title of the heatmap</span>
<span class="k">def</span> <span class="nf">create_heatmap</span><span class="p">(</span><span class="n">df_subset</span><span class="p">,</span> <span class="n">title</span><span class="p">):</span>
    <span class="n">df_subset</span> <span class="o">=</span> <span class="n">df_subset</span><span class="p">[</span><span class="n">df_subset</span><span class="p">[</span><span class="s2">"Profit"</span><span class="p">]</span><span class="o">&gt;</span><span class="mi">0</span><span class="p">]</span><span class="o">.</span><span class="n">reset_index</span><span class="p">()</span>
    <span class="n">df_subset</span> <span class="o">=</span> <span class="n">df_subset</span><span class="p">[</span><span class="n">df_subset</span><span class="p">[</span><span class="s2">"Recipe Cost"</span><span class="p">]</span> <span class="o">&gt;</span> <span class="mi">0</span><span class="p">]</span><span class="o">.</span><span class="n">reset_index</span><span class="p">()</span>
    <span class="c1">#df_subset = df_subset.sort_values(by=["Profit"], ascending=True)</span>
    <span class="n">base_hex_radius</span> <span class="o">=</span> <span class="mi">600</span><span class="o">/</span><span class="n">np</span><span class="o">.</span><span class="n">sqrt</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">df_subset</span><span class="p">))</span><span class="c1"># * 0.5 # 25</span>
    <span class="c1"># Hexagon offset x = radius, y = radius + radius/2</span>
    <span class="n">x_offset</span> <span class="o">=</span> <span class="n">base_hex_radius</span> <span class="o">*</span> <span class="n">np</span><span class="o">.</span><span class="n">sqrt</span><span class="p">(</span><span class="mi">3</span><span class="p">)</span> <span class="c1"># * 0.9</span>
    <span class="n">y_offset</span> <span class="o">=</span> <span class="n">base_hex_radius</span> <span class="o">*</span> <span class="mf">1.5</span><span class="c1"># * 0.9</span>
    <span class="c1"># x_offset,y_offset = base_hex_radius * 5,base_hex_radius*2</span>

    <span class="n">num_points</span> <span class="o">=</span> <span class="nb">len</span><span class="p">(</span><span class="n">df_subset</span><span class="p">)</span>
    <span class="n">num_columns</span> <span class="o">=</span> <span class="nb">int</span><span class="p">(</span><span class="n">np</span><span class="o">.</span><span class="n">ceil</span><span class="p">(</span><span class="n">np</span><span class="o">.</span><span class="n">sqrt</span><span class="p">(</span><span class="n">num_points</span> <span class="o">*</span> <span class="p">(</span><span class="mi">2</span> <span class="o">/</span> <span class="n">np</span><span class="o">.</span><span class="n">sqrt</span><span class="p">(</span><span class="mi">3</span><span class="p">)))))</span>  <span class="c1"># Adjust for hexagonal tiling</span>
