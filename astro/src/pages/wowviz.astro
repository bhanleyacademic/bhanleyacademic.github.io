    <span class="c1"># print(df_subset)</span>

    <span class="n">hexagon</span> <span class="o">=</span> <span class="s2">"M0,-1L0.866,-0.5L0.866,0.5L0,1L-0.866,0.5L-0.866,-0.5Z"</span>
    <span class="n">chart</span> <span class="o">=</span> <span class="n">alt</span><span class="o">.</span><span class="n">Chart</span><span class="p">(</span><span class="n">df_subset</span><span class="p">)</span><span class="o">.</span><span class="n">mark_point</span><span class="p">(</span><span class="n">size</span><span class="o">=</span><span class="n">base_hex_radius</span><span class="o">**</span><span class="mi">2</span><span class="p">,</span> <span class="n">shape</span><span class="o">=</span><span class="n">hexagon</span><span class="p">)</span><span class="o">.</span><span class="n">encode</span><span class="p">(</span>
