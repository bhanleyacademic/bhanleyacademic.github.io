<span class="k">def</span> <span class="nf">get_sample_from_blizzard_api</span><span class="p">(</span><span class="n">api_url</span><span class="p">,</span> <span class="n">filepath</span><span class="p">,</span> <span class="n">access_token</span><span class="o">=</span><span class="n">get_blizz_access_token</span><span class="p">()):</span>
    <span class="n">api_call</span> <span class="o">=</span> <span class="n">get_from_blizzard_api</span><span class="p">(</span><span class="n">api_url</span><span class="p">,</span> <span class="n">access_token</span><span class="p">)</span>
    <span class="k">if</span> <span class="n">api_call</span><span class="p">:</span>
        <span class="n">save_to_json</span><span class="p">(</span><span class="n">api_call</span><span class="p">,</span> <span class="n">filepath</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">api_call</span>


<span class="c1"># Fetch and save Blizzard API JSON</span>
<span class="c1"># @param url: URL to fetch from Blizzard API</span>
<span class="c1"># @param filepath: Filepath to save JSON data</span>
<span class="k">def</span> <span class="nf">fetch_and_save_blizzard_api</span><span class="p">(</span><span class="n">url</span><span class="p">,</span> <span class="n">filepath</span><span class="o">=</span><span class="s1">'professions.json'</span><span class="p">,</span> <span class="n">blizz_token</span><span class="o">=</span><span class="n">get_blizz_access_token</span><span class="p">()):</span>
    <span class="n">api_call</span> <span class="o">=</span> <span class="n">get_from_blizzard_api</span><span class="p">(</span><span class="n">url</span><span class="p">,</span> <span class="n">blizz_token</span><span class="p">)</span>
    <span class="k">if</span> <span class="n">api_call</span><span class="p">:</span>
        <span class="n">save_to_json</span><span class="p">(</span><span class="n">api_call</span><span class="p">,</span> <span class="n">filepath</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">api_call</span>

<span class="c1"># Auction Houses are split between regions for commodities and realm specific for gear and transmog. This function gets the US region commodity auction house data.</span>
<span class="k">def</span> <span class="nf">get_and_save_blizzard_commodity_ah</span><span class="p">():</span>
    <span class="n">url</span> <span class="o">=</span> <span class="s2">"/auctions/commodities"</span>
    <span class="n">date</span> <span class="o">=</span> <span class="n">datetime</span><span class="o">.</span><span class="n">datetime</span><span class="o">.</span><span class="n">now</span><span class="p">()</span><span class="o">.</span><span class="n">strftime</span><span class="p">(</span><span class="s2">"%Y%m</span><span class="si">%d</span><span class="s2">_%H%M"</span><span class="p">)</span>
    <span class="n">filepath</span> <span class="o">=</span> <span class="sa">f</span><span class="s1">'data/auction_data/commodity_auction_data_</span><span class="si">&#123;</span><span class="n">date</span><span class="si">}</span><span class="s1">.json'</span>
    <span class="n">ah</span> <span class="o">=</span> <span class="n">get_from_blizzard_api</span><span class="p">(</span><span class="n">url</span><span class="p">)</span>
    <span class="k">if</span> <span class="n">ah</span><span class="p">:</span>
        <span class="n">save_to_json</span><span class="p">(</span><span class="n">ah</span><span class="p">,</span> <span class="n">filepath</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">ah</span>

<span class="c1"># While realms have their own IDs, they are part of a connected realm. This function gets the connected realm ID for a given realm.</span>
<span class="k">def</span> <span class="nf">get_connected_realm_number</span><span class="p">(</span><span class="n">realm_name</span><span class="p">):</span>
    <span class="n">realm_index</span> <span class="o">=</span> <span class="n">get_from_blizzard_api</span><span class="p">(</span><span class="s2">"/realm/index"</span><span class="p">)</span>
    <span class="n">realm_name</span> <span class="o">=</span> <span class="n">realm_name</span><span class="o">.</span><span class="n">lower</span><span class="p">()</span><span class="o">.</span><span class="n">replace</span><span class="p">(</span><span class="s2">" "</span><span class="p">,</span> <span class="s2">""</span><span class="p">)</span><span class="o">.</span><span class="n">replace</span><span class="p">(</span><span class="s2">"'"</span><span class="p">,</span> <span class="s2">""</span><span class="p">)</span>
    <span class="c1"># print(realm_name)</span>
    <span class="k">for</span> <span class="n">realm</span> <span class="ow">in</span> <span class="n">realm_index</span><span class="p">[</span><span class="s1">'realms'</span><span class="p">]:</span>
        <span class="n">currentRealm</span> <span class="o">=</span> <span class="n">realm</span><span class="p">[</span><span class="s1">'name'</span><span class="p">]</span><span class="o">.</span><span class="n">lower</span><span class="p">()</span><span class="o">.</span><span class="n">replace</span><span class="p">(</span><span class="s2">" "</span><span class="p">,</span> <span class="s2">""</span><span class="p">)</span><span class="o">.</span><span class="n">replace</span><span class="p">(</span><span class="s2">"'"</span><span class="p">,</span> <span class="s2">""</span><span class="p">)</span>
        <span class="k">if</span> <span class="n">currentRealm</span> <span class="o">==</span> <span class="n">realm_name</span><span class="p">:</span>
            <span class="n">href</span> <span class="o">=</span> <span class="n">get_from_blizzard_api</span><span class="p">(</span><span class="sa">f</span><span class="s2">"/realm/</span><span class="si">&#123;</span><span class="n">realm</span><span class="p">[</span><span class="s1">'slug'</span><span class="p">]</span><span class="si">}</span><span class="s2">"</span><span class="p">)[</span><span class="s1">'connected_realm'</span><span class="p">][</span><span class="s1">'href'</span><span class="p">]</span>
            <span class="n">digits</span> <span class="o">=</span> <span class="n">re</span><span class="o">.</span><span class="n">findall</span><span class="p">(</span><span class="sa">r</span><span class="s1">'\d+'</span><span class="p">,</span> <span class="n">href</span><span class="p">)</span>
            <span class="n">result</span> <span class="o">=</span> <span class="s1">''</span><span class="o">.</span><span class="n">join</span><span class="p">(</span><span class="n">digits</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">result</span>
